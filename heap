***
priority queue

This context is similar to a heap, where elements can be inserted at any moment, and only the max heap element can be retrieved
***

CODE

#include <iostream>
#include<queue>
using namespace std;
int main()
{
    priority_queue<int> pq1;
    pq1.push(0);
    pq1.push(2);
    pq1.push(5);
    pq1.push(18);
    pq1.push(10);
    cout<<pq1.size()<<endl;
    cout<<"is empty?: ";
    cout<<pq1.empty()<<endl;
    while(!pq1.empty()){
        cout<<pq1.top()<<endl;
        pq1.pop();
    }
    
    return 0;
}


OUTPUT



5
is empty?: 0
18
10
5
2
0


