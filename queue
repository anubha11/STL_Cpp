***
FIFO
***


CODE

#include <iostream>
#include<queue>
using namespace std;

int main()
{
    queue<int> q1;
    q1.push(0);
    q1.push(2);
    q1.push(5);
    q1.push(8);
    q1.push(10);
    cout<<q1.size()<<endl;
    cout<<"is empty?: ";
    cout<<q1.empty()<<endl;
    cout<<q1.front()<<endl;
    cout<<q1.back()<<endl;
    
    q1.pop();
    cout<<q1.front()<<endl;
    return 0;
}

OUTPUT

5
is empty?: 0
0
10
2
